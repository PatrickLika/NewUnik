@page
@model Unik.WebApp.Pages.Kompetence.EditModel
@{
}
<h2>Rediger Kompetence</h2>
<section class="row">
    <section class="col-md-6">
        <form method="post">

            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <input type="hidden" asp-for="EditViewModel.Id" class="form-control" />
            <input type="hidden" asp-for="EditViewModel.RowVersion" class="form-control" />
            <div class="form-group">
                <label asp-for="EditViewModel.Navn"></label>
                <input asp-for="EditViewModel.Navn" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="EditViewModel.Type"></label>
                <input asp-for="EditViewModel.Type" class="form-control" />
            </div>

            <button type="submit" class="btn btn-default">Opdater Kompetence</button>
        </form>
    </section>
</section>
