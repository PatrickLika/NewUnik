@page
@model Unik.WebApp.Pages.Kompetence.CreateModel
@{
}

<h2>Opret kompetence</h2>
<div class="row">
    <div class="col-md-6">
        <form method="post">
            <div class="form-group">

                <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                <label asp-for="CreateViewModel.Navn"></label>
                <input asp-for="CreateViewModel.Navn" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="CreateViewModel.Type"></label>
                <input asp-for="CreateViewModel.Type" class="form-control" />
                
            </div>

            <button type="submit" class="btn btn-default">Tilføj Kompetence</button>
        </form>
    </div>
</div>