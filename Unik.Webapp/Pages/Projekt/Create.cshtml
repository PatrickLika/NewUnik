@page
@model Unik.WebApp.Pages.Projekt.CreateModel
@{
}

<form method="post">
    <section class="container">
        <section class="row ">
            <section class="col-md-6">
                <fieldset>
                    <br />
                    <h1>Opret projekt</h1>
                    <hr style="height:1px;border:none;color:#000000;background-color:#000000; opacity: 1" />
                    <div class="col-sm-10">
                    </div>
                    <div class="form-floating mb-3">
                        <input type="text" class="form-control" id="floatingInputSalesId" placeholder="Sales Id" asp-for="@Model.ProjektViewModel.SalesId">
                        <label>sælger ID</label>
                    </div>
                    <div class="form-floating mb-3">
                        <input type="text" class="form-control" id="floatingInputKundeId" placeholder="Kunde Id" asp-for="@Model.ProjektViewModel.KundeId">
                        <label >Kunde Id</label>
                    </div>
                    <div class="form-floating mb-3">
                        <input type="text" class="form-control" id="floatingInputNoter" placeholder="Noter" asp-for="@Model.ProjektViewModel.AntalBoliger">
                        <label>Antal boliger</label>
                    </div>

                    <div class="form-floating mb-3">
                        <input type="text" class="form-control" id="floatingInputNoter" placeholder="Noter" asp-for="@Model.ProjektViewModel.Noter">
                        <label>Noter</label>
                    </div>
                </fieldset>
            </section>
            <button type="submit" class="btn btn-success rounded">Opret Projekt</button>
        </section>
        </section>

</form>